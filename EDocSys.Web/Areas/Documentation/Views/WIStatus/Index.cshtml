@{
    ViewData["Title"] = localizer["WIs Progress List"];
    ViewData["Caption"] = localizer[""];
}
@using EDocSys.Web.Areas.Documentation.Models
@model WIStatusViewModel
<div class="card">
    @*@await Component.InvokeAsync("WIDetails", new { Id = 38 })*@
    <div class="col-sm-12" style="padding:20px">
        <h2>WI No: @ViewBag.WSCPNo</h2>
    </div>
    
    <div class="col-sm-12" style="padding:20px">
        <a id="reload" class="btn btn-primary text-white">
            <i class="fa fas fa-bolt"></i>
            Reload
        </a>
    </div>
     <a href="/documentation/wi" class="btn btn-primary"><i class="fas fa-arrow-left"></i>  Back</a>
    <div id="viewAll" class="card-body table-responsive">
    </div>
</div>

@section Scripts
{
    <script src="~/js/site.js"></script>
    <script>
        $(document).ready(function () {
            $("#wiTable").DataTable();
            loadData();
            $('#reload').on('click', function () {
                loadData();
            });
        });
        function loadData() {
            // $('#viewAll').load('/documentation/WIStatus/LoadAll/?wiId = ');
            $('#viewAll').load('/documentation/WIStatus/LoadAll/?wiId='+ '@ViewBag.WIId');
        }
    </script>
}